{
    "layout":
    {
        "fig_width": 4,
        "top_margin": 0,
        "left_margin": 0,
        "right_margin": 0,
        "panel_height": 4,
        "grid_wspace": -0.5
    },
    "x_display":{
        "global_x_field": "subject_type",
        "global_hue": "valvular_disorder",
        "label": "Subjects"
    },
    "formatting":
    {
        "x_label_fontsize": 12,
        "y_label_fontsize": 12,
